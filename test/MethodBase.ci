abstract class Base
{
	internal virtual int Foo() //FAIL: cl
	{
		return 5;
	}

	protected virtual int Quux(int i) => i;
}

class Derived : Base
{
	internal override int Foo()
	{
		return base.Foo() + base.Quux(10);
	}

	internal int Bar()
	{
		return Foo();
	}
}

public static class Test
{
	public static bool Run()
	{
		Derived() d;
		return d.Foo() == 15 && d.Bar() == 15;
	}
}
